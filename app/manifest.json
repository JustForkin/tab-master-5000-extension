{
  "name": "Tab Master 5000: Tab Swiss Army Knife",
  "short_name": "Tab Master 5000",
  "version": "1.4.0",
  "manifest_version": 2,
  "description": "This extensions replaces the new tab page with a clean, sortable, and searchable, grid view of all of your open tabs.",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html",
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Tab Master 5000: Tab and Session Swiss Army Knife"
  },
  "omnibox": {
    "keyword": "new tab"
  },
  "offline_enabled": true,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/content.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage",
    "idle",
    "system.memory",
    "tabCapture",
    "management",
    "bookmarks",
    "history"
  ]
}